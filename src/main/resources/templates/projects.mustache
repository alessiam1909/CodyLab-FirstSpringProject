{{>header}}

<div class="row center-align">
    <h3>Projects list of <strong class="teal-text">{{username}}</strong></h3>
</div>
{{#projects}}

<div class="row">
    <div class="col s12 m6">
        <div class="card  #80cbc4 teal lighten-3">
            <div class="card-image">
                <img src="https://picsum.photos/200/300" height="350px">

            </div>
            <div class="card-content  white-text">
                <span class="card-title">{{name}}</span>
                <p>{{description}}</p>
            </div>
        </div>
    </div>
</div>
{{/projects}}
{{>footer}}